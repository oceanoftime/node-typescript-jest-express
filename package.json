{
  "name": "service",
  "version": "1.0.0",
  "description": "Boilerplate service using Typescript, Jest and Express",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc -p .",
    "start": "npm run build && node -r dotenv/config dist/index.js dotenv_config_path=.prod.env",
    "dev": "nodemon --unhandled-rejections=strict -r dotenv/config dotenv_config_path=.dev.env",
    "lint": "eslint \"./src\"",
    "jest": "jest --verbose",
    "test": "npm run lint && npm run jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/http-server": "^0.10.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "got": "^11.8.1",
    "winston": "^3.3.3"
  }
}
